<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project1.CanManageUserProfileController">
    <children>
        <TextField fx:id="name" layoutX="42.0" layoutY="42.0"  promptText="name" />
        <TextField fx:id="country" layoutX="42.0" layoutY="94.0"  promptText="country" />
        <ComboBox fx:id="conference" layoutX="40.0" layoutY="237.0" prefWidth="150.0" promptText="conference" />
        <Button fx:id="addUserButton" layoutX="40.0" layoutY="310.0" onAction="#addKeySpeakerButtonOnClick" mnemonicParsing="false"  text="Add User" />
        <Button fx:id="removeUserButton" layoutX="304.0" layoutY="273.0" mnemonicParsing="false"  text="Remove User" />
        <TableView layoutX="304.0" layoutY="43.0" prefHeight="200.0" prefWidth="358.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="name" />
                <TableColumn fx:id="countryColumn" prefWidth="75.0" text="country" />
                <TableColumn fx:id="institutionColumn" prefWidth="75.0" text="institution" />
                <TableColumn fx:id="titleColumn" prefWidth="75.0" text="title" />
            </columns>
        </TableView>
        <TextField fx:id="title" layoutX="41.0" layoutY="187.0"  promptText="title" />
        <TextField fx:id="institution" layoutX="42.0" layoutY="137.0"  promptText="institution" />
    </children>
</AnchorPane>
